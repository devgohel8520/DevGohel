@{
    ViewBag.Title = "Contact Me";
}
<script type="text/javascript" src="http://ecn.dev.virtualearth.net/mapcontrol/mapcontrol.ashx?v=6.3"></script>
<div class="content-start">
    <div class="col-lg-12 content-data">
        <div class="row">
            <div class="col-lg-12 content-data-x">
                   <div class="head">
                       DEVENDRA K. GOHEL
                   </div>
                   <div style="font-size:14px;" title="you can find me here now.">
                       <span class="glyphicon glyphicon-globe small"></span> Bodeli, Baroda, Guj, IN.
                   </div>
                   <div style="font-size:14px;">
                       <span class="glyphicon glyphicon-phone-alt small"></span> +91-9909980330
                   </div>
                   <div style="font-size:14px;">
                      <span class="glyphicon glyphicon-envelope small"></span> devsqlpro@gmail.com
                   </div>
            </div>
        </div>
    </div>
    <div class="col-lg-12 content-data nopadding">
        <div class="row">
            <div class="col-lg-12 content-data-x text-center">
                <a class="btn btn-default btn-xs btn-space" href="/devendra">
                    ABOUT ME
                </a>
            </div>
        </div>
    </div>
    <div class="col-lg-12" id="pnlMap" style="height:100vh; width:100%; margin-top:20px;">

    </div>
</div>

@section Scripts
{
    <script type="text/javascript">
        function LoadMap() {
            // Create a new instance of the VEMap Class
            // pnlMap is the ID of the Panel
            var map = new VEMap('pnlMap');
            // Call the VEMap.LoadMap method to load the map
            map.LoadMap();

            try {
                map.Find(null, 'bodeli');
            } catch (e) {
                alert(e.message);
            }
        }

        $(document).ready(function () {
            LoadMap();
        });
    </script> 
}
