@model DevGohel.Models.Sponser

@{
    ViewBag.Title = "Label Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="content-start-admin">
    <div class="row">
        <div class="col-md-12">
            <h4>Delete Sponser</h4>
        </div>
        <div class="col-md-12">
            @using (Ajax.BeginForm("Delete", new AjaxOptions { }))
            {
                <div class="alert-danger error-list">
                    <div id="errorD" style="padding:10px; margin-bottom:20px; display:none;">

                    </div>
                </div>

                @Html.AntiForgeryToken()
                @Html.HiddenFor(model => model.SponserId)
                <div class="form-group input-group input-group-lg">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-copy"></i></span>
                    @Html.EditorFor(model => model.Title, new { htmlAttributes = new { @class = "form-control", @placeholder = "ENTER LABLE NAME" } })
                    @Html.ValidationMessageFor(model => model.Title, "", new { @class = "text-danger form-control-feedback" })
                </div>
                <div class="form-group text-right">
                    <input id="submit" type="button" value="Delete" onclick="submitUniversal(this, '@Url.Action("SIndex", "Sponser")');" class="btn btn-danger" />
                    <a href="@Url.Action("SIndex", "Sponser")" class="btn btn-default">
                        Back
                    </a>
                </div>
            }
        </div>
    </div>
</div>
<div id="scriptadd" class="top-twenty">

</div>
@section Scripts {
    <script>
        $(document).ready(function () {

            $("textarea#Body").on("keyup", function () {
                $("div#scriptadd").html($(this).val());
            });

        });
    </script>
}
